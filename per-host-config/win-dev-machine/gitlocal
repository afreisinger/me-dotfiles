[core]
    # auto-converts lf=>crlf on checkout and crlf=>lf on checkin
    # This makes sense if you are on normal Windows, but NOT in Cygwin
    # autocrlf = true

    # is supposed to make git faster on windows. I have seen no such effect ...
    preloadindex = true
    fscache = true

    # Ignore changes in executable modes, etc
    # Very useful when sharing directories between Cygwin and Windows Git
    fileMode = false
[gc]
